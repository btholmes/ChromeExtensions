{
  "name": "RSS Fetcher",
  "version": "0.1",
  "manifest_version" : 2,
  "description": "Keep up with the latest from [insert web site here].",

  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["jquery-1.7.2.min.js","content.js"],
      "run_at": "document_start"
    }
  ],
        "browser_action": {
    "default_icon": "link.png",
    "default_title": "Spanish Dictionary Word Reader",
    "default_popup": "page.html"
  }
}